{
  "name": "angular-easy-masks",
  "version": "0.2.3",
  "description": "Angular component for input fields based on a configurable mask",
  "main": "release/angular-easy-masks.js",
  "scripts": {
    "build": "./node_modules/.bin/uglifyjs src/masks.js src/directive.js src/filter.js src/module.js -b -e --preamble \"// https://github.com/awerlang/angular-easy-masks\" -o release/angular-easy-masks.js",
    "release": "./node_modules/.bin/uglifyjs src/masks.js src/directive.js src/filter.js src/module.js -c -e --preamble \"// https://github.com/awerlang/angular-easy-masks\" -o release/angular-easy-masks.min.js",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awerlang/angular-easy-masks.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "forms",
    "input",
    "text",
    "mask",
    "masks",
    "ngmodel"
  ],
  "author": "Andr√© Werlang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awerlang/angular-easy-masks/issues"
  },
  "homepage": "https://github.com/awerlang/angular-easy-masks",
  "devDependencies": {
    "conventional-changelog": "0.0.17",
    "jasmine-core": "3.1.0",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.2",
    "uglify-js": "^2.4.20"
  }
